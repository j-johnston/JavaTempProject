public class InvalidTemperatureConverter extends Exception
	{	
		private static final long serialVersionUID = 1L;
		
		private double temp;
		
		public InvalidTemperatureConverter(double t)
		{
			super("Invalid temperature");
			this.temp = t;
		}
	}
